    "type": "git",
    "url": "git+https://github.com/sheinsight/spack.git"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@tsconfig/node-ts": "^23.6.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@types/semver": "^7.7.0",
    "@vitest/ui": "3.2.4",
    "chalk": "^5.4.1",
    "consola": "^3.4.2",
    "enquirer": "^2.4.1",
    "execa": "^9.6.0",
    "find-packages": "^10.0.4",
    "globby": "^14.1.0",
    "read-pkg": "^9.0.1",
    "read-yaml-file": "^2.1.0",
    "semver": "^7.7.2",
    "typescript": "^5.8.3",
    "write-package": "^7.1.0"
  },
