{
  "plugins": [
    "react",
    "unicorn",
    "typescript",
    "oxc"
  ],
  "jsPlugins": null,
  "categories": {
    "correctness": "allow",
    "restriction": "allow",
    "suspicious": "allow",
    "perf": "allow",
    "nursery": "allow",
    "style": "allow",
    "pedantic": "allow"
  },
  "rules": {
    "for-direction": "deny",
    "no-async-promise-executor": "deny",
    "no-caller": "allow",
    "no-class-assign": "deny",
    "no-compare-neg-zero": "deny",
    "no-cond-assign": "deny",
    "no-const-assign": "deny",
    "no-constant-binary-expression": "deny",
    "no-constant-condition": "allow",
    "no-control-regex": "deny",
    "no-debugger": "allow",
    "no-delete-var": "deny",
    "no-dupe-class-members": "deny",
    "no-dupe-else-if": "deny",
    "no-dupe-keys": "deny",
    "no-duplicate-case": "deny",
    "no-empty-character-class": "deny",
    "no-empty-pattern": "deny",
    "no-empty-static-block": "deny",
    "no-eval": [
      "warn",
      [
        {
          "allowIndirect": true
        }
      ]
    ],
    "no-ex-assign": "deny",
    "no-extra-boolean-cast": "warn",
    "no-func-assign": "deny",
    "no-global-assign": "deny",
    "no-import-assign": "deny",
    "no-invalid-regexp": "deny",
    "no-irregular-whitespace": "deny",
    "no-loss-of-precision": "deny",
    "no-new-native-nonconstructor": "deny",
    "no-nonoctal-decimal-escape": "deny",
    "no-obj-calls": "deny",
    "no-self-assign": "allow",
    "no-setter-return": "deny",
    "no-shadow-restricted-names": "deny",
    "no-sparse-arrays": "allow",
    "no-this-before-super": "deny",
    "no-unassigned-vars": "allow",
    "no-unsafe-finally": "deny",
    "no-unsafe-negation": [
      "deny",
      [
        {
          "enforceForOrderingRelations": false
        }
      ]
    ],
    "no-unsafe-optional-chaining": "deny",
    "no-unused-expressions": "allow",
    "no-unused-labels": "allow",
    "no-unused-private-class-members": "allow",
    "no-unused-vars": "allow",
    "no-useless-backreference": "deny",
    "no-useless-catch": "deny",
    "no-useless-escape": "warn",
    "no-useless-rename": "deny",
    "no-with": "allow",
    "require-yield": "deny",
    "use-isnan": "deny",
    "valid-typeof": "deny",
    "no-await-in-loop": "deny",
    "no-useless-call": "allow",
    "class-methods-use-this": "allow",
    "default-case": "allow",
    "no-alert": "warn",
    "no-bitwise": "allow",
    "no-console": "warn",
    "no-div-regex": "warn",
    "no-empty": "warn",
    "no-empty-function": [
      "warn",
      [
        {
          "allow": [
            "constructors",
            "arrowFunctions"
          ]
        }
      ]
    ],
    "no-eq-null": "deny",
    "no-iterator": "deny",
    "no-param-reassign": "deny",
    "no-plusplus": "allow",
    "no-proto": "deny",
    "no-regex-spaces": "deny",
    "no-restricted-globals": [
      "deny",
      [
        {
          "name": "event",
          "message": "Use local parameter instead."
        }
      ]
    ],
    "no-restricted-imports": [
      "deny",
      [
        {
          "paths": [
            {
              "name": "import-foo",
              "message": "Please use import-bar instead."
            },
            {
              "name": "import-baz",
              "message": "Please use import-quux instead."
            }
          ]
        }
      ]
    ],
    "no-undefined": "warn",
    "no-var": "deny",
    "no-void": "allow",
    "unicode-bom": "deny",
    "block-scoped-var": "deny",
    "no-extend-native": "deny",
    "no-extra-bind": "deny",
    "no-new": "warn",
    "no-unexpected-multiline": "deny",
    "no-unneeded-ternary": "warn",
    "no-useless-concat": "deny",
    "no-useless-constructor": "allow",
    "preserve-caught-error": [
      "deny",
      [
        {
          "requireCatchParameter": true
        }
      ]
    ],
    "array-callback-return": "warn",
    "eqeqeq": [
      "deny",
      [
        "always",
        {
          "null": "always"
        }
      ]
    ],
    "max-classes-per-file": [
      "deny",
      [
        {
          "max": 1
        }
      ]
    ],
    "max-depth": "allow",
    "max-lines": [
      "deny",
      [
        {
          "max": 1000,
          "skipBlankLines": true,
          "skipComments": true
        }
      ]
    ],
    "max-lines-per-function": [
      "deny",
      [
        {
          "max": 300,
          "skipBlankLines": true,
          "skipComments": true,
          "IIFEs": false
        }
      ]
    ],
    "max-nested-callbacks": [
      "deny",
      [
        {
          "max": 3
        }
      ]
    ],
    "no-array-constructor": "deny",
    "no-case-declarations": "deny",
    "no-constructor-return": "deny",
    "no-else-return": "allow",
    "no-fallthrough": "allow",
    "no-inner-declarations": "allow",
    "no-lonely-if": "deny",
    "no-negated-condition": "allow",
    "no-new-wrappers": "deny",
    "no-object-constructor": "deny",
    "no-prototype-builtins": "deny",
    "no-redeclare": [
      "deny",
      [
        {
          "builtinGlobals": true
        }
      ]
    ],
    "no-self-compare": "deny",
    "no-throw-literal": "deny",
    "radix": "allow",
    "require-await": "deny",
    "sort-vars": "allow",
    "symbol-description": "deny",
    "getter-return": "deny",
    "no-misleading-character-class": "allow",
    "no-undef": "deny",
    "no-unreachable": "deny"
  },
  "settings": {
    "jsx-a11y": {
      "polymorphicPropName": null,
      "components": {},
      "attributes": {}
    },
    "next": {
      "rootDir": []
    },
    "react": {
      "formComponents": [],
      "linkComponents": []
    },
    "jsdoc": {
      "ignorePrivate": false,
      "ignoreInternal": false,
      "ignoreReplacesDocs": true,
      "overrideReplacesDocs": true,
      "augmentsExtendsReplacesDocs": false,
      "implementsReplacesDocs": false,
      "exemptDestructuredRootsFromChecks": false,
      "tagNamePreference": {}
    }
  },
  "env": {
    "browser": true,
    "es2024": true
  },
  "globals": {},
  "ignorePatterns": []
}